    "test": "jest --watch",
    "updateSnapshot": "jest --updateSnapshot",
    "coverage": "jest --coverage",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx components/",
    "lint:fix": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx components/ --fix",
